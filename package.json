{
  "name": "telegram-weather-bot",
  "version": "1.0.0",
  "description": "Telegram Bot for search your location weather.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "lint": "eslint src",
    "format": "eslint src --fix",
    "test": "npm run lint && ava -v",
    "test:cov": "NODE_ENV=test nyc npm test"
  },
  "keywords": [
    "telegram",
    "bot",
    "api",
    "weather"
  ],
  "author": "Peng-Jie <dean.leehom@gmail.com> (https://github.com/neighborhood999)",
  "repository": {
    "type": "git",
    "url": "https://github.com/neighborhood999/telegram-weather-bot"
  },
  "ava": {
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "files": "test/**/*.spec.js",
    "babel": "inherit"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "eslint": "^4.8.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-prettier": "^2.3.1",
    "nyc": "^11.2.1",
    "prettier": "^1.7.4"
  },
  "dependencies": {
    "date-fns": "^1.28.5",
    "dotenv": "^4.0.0",
    "isomorphic-fetch": "^2.2.1",
    "node-telegram-bot-api": "^0.28.0"
  },
  "license": "MIT"
}
